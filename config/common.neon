# Application parameters and settings. See https://doc.nette.org/configuring

parameters:


application:
	# Error presenter to handle application errors
	errorPresenter: Auxiliary:Error
	# Presenter mapping pattern
	mapping:
		*: App\Module\*\Presenters\*Presenter


session:
	expiration: 14 days

di:
	export:
		parameters: no
		tags: no



security:
	users:
		admin: secret  # user 'admin', password 'secret'
		user: secret # user 'furry', password 'ilovegays'